<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<!-- Vulgar getting insulted -->
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThoughtDef[defName="Insulted"]</xpath>
		<value>
			<nullifyingTraits>
				<li>BOT_Vulgar</li>
			</nullifyingTraits>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationAdd">
		<xpath>/Defs/ThoughtDef[defName="InsultedMood"]</xpath>
		<value>
			<nullifyingTraits>
				<li>BOT_Vulgar</li>
			</nullifyingTraits>
		</value>
	</Operation>
</Patch>
